<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>WebVTTStreamReader</PackageId>
    <Version>1.0.0</Version>
    <Title>WebVTTStreamReader</Title>
    <Authors>Nathan Wurtz</Authors>
    <TargetFramework>net5.0</TargetFramework>
    <Description>
      This package is a subtitle IPTV stream reader, it allows you to get the subtitle of a IPTV channel as they are received by the program.
      More information here :
      https://github.com/nate-1/WebVTTStreamReader
    </Description>
    <Summary>This package is a subtitle IPTV stream reader</Summary>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>
</Project>
